a=read.table('test.graph',sep='\t')
count=as.data.frame(table(a$V3))
a=a[a$V3=='binding',]
library(pracma)
all_primes=primes(1000000)
gene=c(as.character(a$V1),as.character(a$V2))
gene=unique(gene)
checks=data.frame(V1=gene,id=all_primes[1:length(gene)])
rownames(checks)=checks$V1
View(checks)
a$V5=checks[as.character(a$V1),'id']
a$V6=checks[as.character(a$V2),'id']
a$V7=a$V5*a$V6
dele=as.data.frame(table(a$V7))
View(dele)
